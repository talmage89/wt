{
  "packageManager": "pnpm@10.30.1",
  "name": "wt",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "wt": "./bin/wt.mjs"
  },
  "main": "./dist/cli.js",
  "scripts": {
